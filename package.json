{
  "name": "BankRollerApp",
  "version": "1.0.0",
  "description": "Dao.Casino app for bankroller",
  "main": "./build/index.electron.js",
  "homepage": "./",
  "repository": "https://github.com/DaoCasino/",
  "author": "Alex Step <to@alexstep.com>",
  "private": true,
  "scripts": {
    "start": "npm run serve:web",
    "serve:web": "node scripts/start.js",
    "build:web": "node scripts/build.js",
    "build": "npm run cp_electron & npm run build:web",
    "cp_electron": "cp ./src/index.electron.js ./public/index.electron.js & cp ./src/index.electron.js ./build/index.electron.js",
    "serve:electron": "npm run cp_electron & npm run serve:web & sleep 5; export dev=true; electron .",
    "run:electron": "npm run cp_electron; electron .",
    "build:electron": "build -mwl --x64 --ia32",
    "build:electron:windows": "build -w --x64",
    "pack:electron:linux": "npm run cp_electron & electron-packager ./ BankRoller --overwrite --app-version=1.0.0 --out=dist --ignore=node_modules --ignore=src --ignore=config --ignore=scripts --ignore=public --prune",
    "pack:electron:windows": "npm run cp_electron & electron-packager ./ BankRoller --overwrite --platform=win32 --app-version=1.0.0 --out=dist --ignore=node_modules --ignore=src --ignore=config --ignore=scripts --ignore=public --prune",
    "pack:electron": "npm run cp_electron & electron-packager ./ BankRoller --overwrite --all --app-version=1.0.0 --out=dist --ignore=node_modules --ignore=src --ignore=config --ignore=scripts --ignore=public --prune",
    "test": "node scripts/test.js --env=jsdom"
  },
  "build": {
    "appId": "com.daocasino.bankroller",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!config/**/*",
      "!scripts/**/*",
      "!src/**/*",
      "!builds/**/*",
      "package.json"
    ],
    "publish": null,
    "win": {
      "icon": "public/windows-icon.ico"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,tag}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|tag)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(styl|sass|less|css)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|tag|styl|sass|less|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|tag)$"
    ]
  },
  "babel": {
    "presets": []
  },
  "eslintConfig": {},
  "dependencies": {
    "async-priority-queue": "^0.1.2",
    "big-integer": "^1.6.22",
    "eth-lightwallet": "^2.5.4",
    "ethereumjs-abi": "^0.6.4",
    "jquery": "^3.2.1",
    "localforage": "^1.5.0",
    "qrcode-svg": "^0.9.0",
    "riot": "^3.5.1",
    "riot-route": "^3.1.1",
    "solidity-sha3": "^0.4.1",
    "web3": "^0.19.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "electron": "^1.6.7",
    "electron-builder": "^17.10.0",
    "electron-packager": "^8.7.0",
    "eslint": "3.8.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-riot": "^0.1.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.0",
    "object-assign": "4.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "0.4.2",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "riotjs-loader": "^4.0.0",
    "sass-loader": "^4.0.2",
    "serviceworker-webpack-plugin": "^0.2.1",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  }
}
